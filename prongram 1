#include <iostream>

int main() {
    // Deklarasi variabel
    int angka1, angka2, hasil;

    // Input dari pengguna
    std::cout << "Masukkan angka pertama: ";
    std::cin >> angka1;

    std::cout << "Masukkan angka kedua: ";
    std::cin >> angka2;

    // Proses penjumlahan
    hasil = angka1 + angka2;

    // Output hasil
    std::cout << "Hasil penjumlahan: " << hasil << std::endl;

    return 0;
}
